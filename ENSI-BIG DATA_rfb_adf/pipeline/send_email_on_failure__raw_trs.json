{
	"name": "send_email_on_failure__raw_trs",
	"properties": {
		"description": "Based on the process for \"Educação SENAI\"\n\nAll parameters point to inexisting objects in ADLS and in Databricks.",
		"activities": [
			{
				"name": "trusted__0__switch_env",
				"description": "Calls trusted template that switches env.",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "trusted__0__switch_env",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"env": {
							"value": "@pipeline().parameters.env",
							"type": "Expression"
						},
						"tables": {
							"value": "@pipeline().parameters.tables",
							"type": "Expression"
						},
						"databricks": {
							"value": "@pipeline().parameters.databricks",
							"type": "Expression"
						},
						"adf": {
							"value": "{'adf_factory_name': '@{pipeline().DataFactory}', 'adf_pipeline_name': '@{pipeline().Pipeline}', 'adf_pipeline_run_id': '@{pipeline().RunId}',  'adf_trigger_id': '@{pipeline().TriggerId}', 'adf_trigger_name': '@{pipeline().TriggerName}', 'adf_trigger_time': '@{pipeline().TriggerTime}',  'adf_trigger_type': '@{pipeline().TriggerType}'}",
							"type": "Expression"
						}
					}
				}
			}
		],
		"concurrency": 1,
		"parameters": {
			"tables": {
				"type": "object",
				"defaultValue": {
					"origins": [
						"/inexisting_origin"
					],
					"destination": "/inexisting_destination"
				}
			},
			"databricks": {
				"type": "object",
				"defaultValue": {
					"trs": {
						"notebook": "/inexisting_notebook"
					}
				}
			},
			"user_parameters": {
				"type": "object",
				"defaultValue": {
					"null": "null"
				}
			},
			"env": {
				"type": "object",
				"defaultValue": {
					"env": "dev"
				}
			}
		},
		"folder": {
			"name": "test/send_email"
		},
		"annotations": [
			"trusted"
		]
	}
}