{
	"name": "raw_trs_unidade_atendimento_caracteristica",
	"properties": {
		"description": "Run trusted for \"Educação SENAI\" from raw tables of bd_basi\n\nNo parameters is required. This table does not implement version control, only updates the records with the most recent value. \n\nPaths are:\nvar_tables =  \"origins\": [\"/bdo/bd_basi/tb_unidade_atendimento\",\"/bdo/oba/tb_industria_conhecimento\"],\"destination\": \"/mtd/corp/unidade_atendimento_caracteristica\"}",
		"activities": [
			{
				"name": "trusted__0__switch_env",
				"description": "Calls trusted template that switches env.",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "trusted__0__switch_env",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"env": {
							"value": "@pipeline().parameters.env",
							"type": "Expression"
						},
						"tables": {
							"value": "@pipeline().parameters.tables",
							"type": "Expression"
						},
						"databricks": {
							"value": "@pipeline().parameters.databricks",
							"type": "Expression"
						},
						"adf": {
							"value": "{'adf_factory_name': '@{pipeline().DataFactory}', 'adf_pipeline_name': '@{pipeline().Pipeline}', 'adf_pipeline_run_id': '@{pipeline().RunId}',  'adf_trigger_id': '@{pipeline().TriggerId}', 'adf_trigger_name': '@{pipeline().TriggerName}', 'adf_trigger_time': '@{pipeline().TriggerTime}',  'adf_trigger_type': '@{pipeline().TriggerType}'}",
							"type": "Expression"
						}
					}
				}
			}
		],
		"concurrency": 1,
		"parameters": {
			"tables": {
				"type": "object",
				"defaultValue": {
					"origins": [
						"/bdo/bd_basi/tb_unidade_atendimento",
						"/bdo/oba/tb_industria_conhecimento"
					],
					"destination": "/mtd/corp/unidade_atendimento_caracteristica"
				}
			},
			"databricks": {
				"type": "object",
				"defaultValue": {
					"trs": {
						"notebook": "/trs/educacao_senai/raw_trs_unidade_atendimento_caracteristica"
					}
				}
			},
			"user_parameters": {
				"type": "object",
				"defaultValue": {
					"null": "null"
				}
			},
			"env": {
				"type": "object",
				"defaultValue": {
					"env": "dev"
				}
			}
		},
		"folder": {
			"name": "trs/educacao_senai"
		},
		"annotations": [
			"trusted"
		]
	}
}